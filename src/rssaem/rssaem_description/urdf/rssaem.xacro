  <link name="camera">
    <collision>
      <origin xyz="-0.01 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="-0.01 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.02 0.02 0.02"/>
      </geometry>
      <material name="green"/>
    </visual>

    <visual>
      <origin xyz="0 0 0" rpy="0 1.5708 0"/>
      <geometry>
        <cylinder length="0.01" radius="0.0063" />
      </geometry>
    </visual>
    
    <inertial>
      <mass value="0.025" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <box_inertia m="0.025" x="0.02" y="0.02" z="$0.02" />
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>

  <joint name="camera_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz=".075 0 .082" rpy="0 .3 0"/>
    <parent link="base_link"/>
    <child link="camera"/>
  </joint>

  <!-- Laser -->

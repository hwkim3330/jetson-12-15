   <joint name="caster_front_joint" type="fixed">
    <axis xyz="0 0 0" />
    <origin xyz=".058 0 -0.01049" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="caster_front"/>
  </joint>
  
  <link name="caster_front">
    <collision name='caster_front_rod_collision'>
      <origin xyz="0 0 0.003" rpy=" 0 0 0"/>
      <geometry>
        <cylinder radius="0.008" length="0.012"/>
      </geometry>
    </collision>
    
    <visual name='caster_front_rod_visual'>
      <origin xyz="0 0 0.003" rpy=" 0 0 0"/>
      <geometry>
        <cylinder radius="0.008" length="0.012"/>
      </geometry>
      <material name="grey"/>
    </visual>
      
    <collision name='caster_front_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.006"/>
      </geometry>
      <material name="black"/>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='caster_front_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.006"/>
      </geometry>
      <material name="gray"/>
    </visual>
  </link>
  
